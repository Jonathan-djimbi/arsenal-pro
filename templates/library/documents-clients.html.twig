{% extends 'library/header_library.html.twig' %}

{% block title %}<title>{{titre}}</title>{% endblock %}

{% block content %}
    {{ include("library/recherche_documents.html.twig")}}
    <div class="bibliothequeAfficherFichiers">
        {% for key, ae in src|sort %}
            <div class="d-flex justify-content-center">
                <p></p><a href="{{ path('app_library_documents_clients_one', { 'src' : ae.src}) }}">{{ae.src}}</a>
            </div>
        {% endfor %}
      </div>
{% endblock %}